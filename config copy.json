{
    "url": "https://example.com",
    "method": "GET",
    "headers": {
        "User-Agent": "Mozilla/5.0 (compatible; scrapie/1.0)"
    },
    "data_selectors": {
        "title": {
            "selector": "title",
            "type": "text"
        },
        "header": {
            "selector": "h1",
            "type": "text"
        },
        "first_link": {
            "selector": "a",
            "type": "attribute",
            "attribute": "href"
        },
        "all_paragraphs": {
            "selector": "p",
            "type": "texts"
        }
    }
}
